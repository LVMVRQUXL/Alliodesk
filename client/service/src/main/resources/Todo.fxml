<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ToDoList.ToDoController">
   <items>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="65.0" prefWidth="600.0">
         <children>
            <Button fx:id="addButton" layoutX="22.0" layoutY="20.0" mnemonicParsing="false" onAction="#addEvent" text="Add ">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </Button>
            <DatePicker fx:id="datePicker" layoutX="99.0" layoutY="20.0" prefHeight="25.0" prefWidth="120.0" />
            <TextField fx:id="descriptionTestField" layoutX="245.0" layoutY="21.0" prefHeight="25.0" prefWidth="332.0" promptText="Add ToDo description..." />
            <Separator layoutX="228.0" layoutY="3.0" orientation="VERTICAL" prefHeight="59.0" prefWidth="5.0" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="230.0" AnchorPane.topAnchor="3.0" />
            <Separator layoutX="93.0" orientation="VERTICAL" prefHeight="65.0" prefWidth="6.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="85.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <ListView fx:id="eventList" prefHeight="200.0" prefWidth="200.0" />
   </items>
</SplitPane>
